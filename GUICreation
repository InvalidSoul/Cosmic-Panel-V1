-- Module Scripts:

local fake_module_scripts = {}

do -- nil.DraggableObject
	local script = Instance.new('ModuleScript', nil)
	script.Name = "DraggableObject"
	local function module_script()
		--[[
			@Author: Spynaz
			@Description: Enables dragging on GuiObjects. Supports both mouse and touch.
			
			For instructions on how to use this module, go to this link:
			https://devforum.roblox.com/t/simple-module-for-creating-draggable-gui-elements/230678
		--]]
		
		local UDim2_new = UDim2.new
		
		local UserInputService = game:GetService("UserInputService")
		
		local DraggableObject 		= {}
		DraggableObject.__index 	= DraggableObject
		
		-- Sets up a new draggable object
		function DraggableObject.new(Object)
			local self 			= {}
			self.Object			= Object
			self.DragStarted	= nil
			self.DragEnded		= nil
			self.Dragged		= nil
			self.Dragging		= false
			
			setmetatable(self, DraggableObject)
			
			return self
		end
		
		-- Enables dragging
		function DraggableObject:Enable()
			local object			= self.Object
			local dragInput			= nil
			local dragStart			= nil
			local startPos			= nil
			local preparingToDrag	= false
			
			-- Updates the element
			local function update(input)
				local delta 		= input.Position - dragStart
				local newPosition	= UDim2_new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
				object.Position 	= newPosition
			
				return newPosition
			end
			
			self.InputBegan = object.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
					preparingToDrag = true
					--[[if self.DragStarted then
						self.DragStarted()
					end
					
					dragging	 	= true
					dragStart 		= input.Position
					startPos 		= Element.Position
					--]]
					
					local connection 
					connection = input.Changed:Connect(function()
						if input.UserInputState == Enum.UserInputState.End and (self.Dragging or preparingToDrag) then
							self.Dragging = false
							connection:Disconnect()
							
							if self.DragEnded and not preparingToDrag then
								self.DragEnded()
							end
							
							preparingToDrag = false
						end
					end)
				end
			end)
			
			self.InputChanged = object.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
					dragInput = input
				end
			end)
			
			self.InputChanged2 = UserInputService.InputChanged:Connect(function(input)
				if object.Parent == nil then
					self:Disable()
					return
				end
				
				if preparingToDrag then
					preparingToDrag = false
					
					if self.DragStarted then
						self.DragStarted()
					end
					
					self.Dragging	= true
					dragStart 		= input.Position
					startPos 		= object.Position
				end
				
				if input == dragInput and self.Dragging then
					local newPosition = update(input)
					
					if self.Dragged then
						self.Dragged(newPosition)
					end
				end
			end)
		end
		
		-- Disables dragging
		function DraggableObject:Disable()
			self.InputBegan:Disconnect()
			self.InputChanged:Disconnect()
			self.InputChanged2:Disconnect()
			
			if self.Dragging then
				self.Dragging = false
				
				if self.DragEnded then
					self.DragEnded()
				end
			end
		end
		
		return DraggableObject
		
	end
	fake_module_scripts[script] = module_script
end


-- Scripts:

local function FBFHY_fake_script() -- CosmicPanel.MainHandler 
	local script = Instance.new('LocalScript', CosmicPanel)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	-- [[DRAGGABLE/MAIN VARIABLES]] --
	
	local Player = game.Players.LocalPlayer
	local CosmicPanelEvent = game.ReplicatedStorage:WaitForChild("CosmicPanelRemoteEvent", math.huge)
	local CosmicPanel = script.Parent.Parent:WaitForChild("Cosmic Panel", math.huge)
	
	local DragAPI = require(script:WaitForChild("DraggableObject", math.huge))
	local DragFrame = DragAPI.new(script.Parent:WaitForChild("MainBackround", math.huge))
	
	local SearchBar = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("SearchUser", math.huge):WaitForChild("SearchUserText", math.huge)
	local HeadImage = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("UserInfoPanel", math.huge):WaitForChild("HeadShot", math.huge):WaitForChild("HeadShotImage", math.huge)
	local UserName = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("UserInfoPanel", math.huge):WaitForChild("Name", math.huge):WaitForChild("NameLabel", math.huge)
	local UserID = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("UserInfoPanel", math.huge):WaitForChild("ID", math.huge):WaitForChild("IDLabel", math.huge)
	local UserAdmin = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("UserInfoPanel", math.huge):WaitForChild("Admin", math.huge):WaitForChild("AdminLabel", math.huge)
	local UserWhitelist = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("UserInfoPanel", math.huge):WaitForChild("Whitelisted", math.huge):WaitForChild("WhitelistedLabel", math.huge)
	local UserWarns = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("UserInfoPanel", math.huge):WaitForChild("Warns", math.huge):WaitForChild("WarnsLabel", math.huge)
	local UserBanned = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround"):WaitForChild("UserInfoPanel"):WaitForChild("Banned"):WaitForChild("BannedLabel")
	local UserBanLength = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("UserInfoPanel", math.huge):WaitForChild("BanLength", math.huge):WaitForChild("BanLengthLabel", math.huge)
	local ClearWarnsButton = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("UserInfoPanel", math.huge):WaitForChild("ClearWarns", math.huge):WaitForChild("ClearWarnsButton", math.huge)
	local UnbanButton = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("SecondaryBackround", math.huge):WaitForChild("UserInfoPanel", math.huge):WaitForChild("Unban", math.huge):WaitForChild("UnbanButton", math.huge)
	local RefreshButton = CosmicPanel:WaitForChild("MainBackround", math.huge):WaitForChild("TitleBackround", math.huge):WaitForChild("Refresh", math.huge):WaitForChild("RefreshButton", math.huge)
	
	DragFrame:Enable()
	
	-- [[DISABLE/ENABLED GUI]] --
	
	local UserInputService = game:GetService("UserInputService")
	local EnableDisableKey = Enum.KeyCode.Insert
	local TweenService = game:GetService("TweenService")
	local Fade = TweenInfo.new(.25)
	
	local Clicked = false
	local ClearWarnClicked = false
	local UnbanClickd = false
	
	local function EnableGui()
		CosmicPanel.Enabled = true
		for _, ImageLabels in pairs(CosmicPanel:GetDescendants()) do
			if ImageLabels:IsA("ImageLabel") then
				local DisableImageLabels = TweenService:Create(ImageLabels, Fade, {ImageTransparency = 0}):Play()
			end
		end
		for _, Frames in pairs(CosmicPanel:GetDescendants()) do
			if Frames:IsA("Frame") then
				local DisableImageLabels = TweenService:Create(Frames, Fade, {BackgroundTransparency = 0}):Play()
			end
		end
		for _, TextLabels in pairs(CosmicPanel:GetDescendants()) do
			if TextLabels:IsA("TextLabel") or TextLabels:IsA("TextBox") then
				local TextLabels = TweenService:Create(TextLabels, Fade, {TextTransparency = 0}):Play()
			end
		end
		for _, TextStroke in pairs(CosmicPanel:GetDescendants()) do
			if TextStroke:IsA("UIStroke") then
				local TextStroke = TweenService:Create(TextStroke, Fade, {Transparency = 0}):Play()
			end
		end
	end
	
	local function DisableGui()
		for _, ImageLabels in pairs(CosmicPanel:GetDescendants()) do
			if ImageLabels:IsA("ImageLabel") then
				local DisableImageLabels = TweenService:Create(ImageLabels, Fade, {ImageTransparency = 1}):Play()
			end
		end
		for _, Frames in pairs(CosmicPanel:GetDescendants()) do
			if Frames:IsA("Frame") then
				local DisableImageLabels = TweenService:Create(Frames, Fade, {BackgroundTransparency = 1}):Play()
			end
		end
		for _, TextLabels in pairs(CosmicPanel:GetDescendants()) do
			if TextLabels:IsA("TextLabel") or TextLabels:IsA("TextBox") then
				local TextLabels = TweenService:Create(TextLabels, Fade, {TextTransparency = 1}):Play()
			end
		end
		for _, TextStroke in pairs(CosmicPanel:GetDescendants()) do
			if TextStroke:IsA("UIStroke") then
				local TextStroke = TweenService:Create(TextStroke, Fade, {Transparency = 1}):Play()
			end
		end
		task.wait(.25)
		CosmicPanel.Enabled = false
		SearchBar.Text = "Search by ID/Username"
		HeadImage.Image = ""
		UserName.Text = "User:"
		UserID.Text = "ID:"
		UserAdmin.Text = "Admin:"
		UserWhitelist.Text = "Whitelisted:"
		UserWarns.Text = "Warns:"
		UserBanned.Text = "Banned:"
		UserBanLength.Text = "Ban Length:"
		ClearWarnsButton.Parent.ClearWarnsLabel.TextColor3 = Color3.fromRGB(255, 115, 115)
		UnbanButton.Parent.UnbanLabel.TextColor3 = Color3.fromRGB(255, 115, 115)
	end
	
	UserInputService.InputBegan:Connect(function(Key, GPE)
		if not Clicked then 
			Clicked = true
			if Key.KeyCode == EnableDisableKey and CosmicPanel.Enabled == true then
				DisableGui()
			else if Key.KeyCode == EnableDisableKey and CosmicPanel.Enabled == false then
					EnableGui()
				end
			end
		end
		task.wait(.25)
		if Clicked then
			Clicked = false
		end
	end)
	
	-- [[REFRESH BUTTON]] --
	
	RefreshButton.MouseButton1Down:Connect(function()
	
		SearchBar.Text = "Search by ID/Username"
		HeadImage.Image = ""
		UserName.Text = "User:"
		UserID.Text = "ID:"
		UserAdmin.Text = "Admin:"
		UserWhitelist.Text = "Whitelisted:"
		UserWarns.Text = "Warns:"
		UserBanned.Text = "Banned:"
		UserBanLength.Text = "Ban Length:"
		ClearWarnsButton.Parent.ClearWarnsLabel.TextColor3 = Color3.fromRGB(255, 115, 115)
		UnbanButton.Parent.UnbanLabel.TextColor3 = Color3.fromRGB(255, 115, 115)
	end)
	
	-- [[MAIN SEARCH HANDLER]] --
	
	SearchBar.FocusLost:Connect(function(PressedEnter) 
		if PressedEnter then
			CosmicPanelEvent:FireServer(SearchBar.Text, "Search")
		end
	end)
	
	CosmicPanelEvent.OnClientEvent:Connect(function(Username, ID, Admin, Whitelist, Warnings, Banned, BanLength)
		HeadImage.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=+"..ID.."+&width=420&height=420&format=png"
		UserName.Text = "User: "..game.Players:GetNameFromUserIdAsync(Username)
		UserID.Text = "ID: "..ID
		if Admin == nil then
			UserAdmin.Text = "Admin: No"
		else if Admin == true then
				UserAdmin.Text = "Admin: Yes"
			end
		end
		if Whitelist == nil then
			UserWhitelist.Text = "Whitelisted: No"
		else if Whitelist == true then
				UserWhitelist.Text = "Whitelisted: Yes"
			end
		end
		UserWarns.Text = "Warnings: "..Warnings
		if Banned == nil then
			UserBanned.Text = "Banned: No"
		else if Banned == true then
				UserBanned.Text = "Banned: Yes"
			end
		end
		UserBanLength.Text = "Ban Length: "..BanLength
		if tonumber(Warnings) > 0 then
			ClearWarnsButton.Parent.ClearWarnsLabel.TextColor3 = Color3.fromRGB(129, 255, 115)
		end
		if Banned then
			UnbanButton.Parent.UnbanLabel.TextColor3 = Color3.fromRGB(129, 255, 115)
		end
	end)
	
	-- [[ CLEAR WARN HANDLER ]] --
	
	ClearWarnsButton.MouseButton1Down:Connect(function()
		if ClearWarnsButton.Parent.ClearWarnsLabel.TextColor3 == Color3.fromRGB(129, 255, 115) then
			local CanClear = ClearWarnsButton:GetAttribute("CanClear")
			if not CanClear then
				ClearWarnsButton.Parent.ClearWarnsLabel.Text = "Click Again To Clear Warns"
				ClearWarnsButton:SetAttribute("CanClear", true)
			else if CanClear then
					ClearWarnsButton.Parent.ClearWarnsLabel.TextColor3 = Color3.fromRGB(255, 115, 115)
					ClearWarnsButton:SetAttribute("CanClear", false)
					ClearWarnsButton.Parent.ClearWarnsLabel.Text = "Warns Cleared"
					CosmicPanelEvent:FireServer(SearchBar.Text, "Clear Warns")
					task.wait(2)
					CosmicPanelEvent:FireServer(SearchBar.Text, "Search")
					ClearWarnsButton.Parent.ClearWarnsLabel.Text = "Clear Warns"
				end
			end
		end
	end)
	
	UnbanButton.MouseButton1Down:Connect(function()
		if UnbanButton.Parent.UnbanLabel.TextColor3 == Color3.fromRGB(129, 255, 115) then
			local CanClear = UnbanButton:GetAttribute("CanUnban")
			if not CanClear then
				UnbanButton.Parent.UnbanLabel.Text = "Click Again To Unban"
				UnbanButton:SetAttribute("CanUnban", true)
			else if CanClear then
					UnbanButton.Parent.UnbanLabel.TextColor3 = Color3.fromRGB(255, 115, 115)
					UnbanButton:SetAttribute("CanUnban", false)
					UnbanButton.Parent.UnbanLabel.Text = "Warns Cleared"
					CosmicPanelEvent:FireServer(SearchBar.Text, "Unban")
					task.wait(2)
					CosmicPanelEvent:FireServer(SearchBar.Text, "Search")
					UnbanButton.Parent.UnbanLabel.Text = "Clear Warns"
				end
			end
		end
	end)
end
coroutine.wrap(FBFHY_fake_script)()
